{
    "no_url_scheme": {
      "rules": [{
          "matches": [
             {"name": "url-wildcard",
              "value": "/*"}
         ]
         }
       ]
    },
    "url_scheme_http": {
      "rules": [{
         "matches": [
            {"name": "url-wildcard",
             "value":  "/*"},
            {"name": "url-scheme",
             "value": "http"}
         ]}
      ]
    },
    "url_scheme_https": {
       "rules": [{
          "matches": [
            {"name": "url-wildcard",
             "value": "/*"},
            {"name": "url-scheme",
             "value": "https"}
       ]}
       ]
    }
}
